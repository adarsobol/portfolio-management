# Files and directories to ignore when deploying to Google Cloud

# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Build outputs (will be built in Cloud Build)
dist/
build/
*.log

# Environment files (secrets should be in Secret Manager)
.env
.env.local
.env.*.local
.env.production
.env.development

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Testing
coverage/
.nyc_output/
*.test.ts
*.test.tsx
*.spec.ts
*.spec.tsx
__tests__/
e2e/
playwright-report/
test-results/

# Documentation (not needed in production)
docs/
*.md
!README.md

# Git
.git/
.gitignore
.gitattributes

# CI/CD (not needed in container)
.github/
.gitlab-ci.yml
.travis.yml

# Misc
*.zip
*.tar.gz
.cache/
.temp/
tmp/

# TypeScript build info
*.tsbuildinfo

# Local development files
.vite/
playwright.config.ts
vitest.config.ts

